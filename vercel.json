{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/email-configure",
      "destination": "/api/email-configure.js"
    },
    {
      "source": "/api/email-status",
      "destination": "/api/email-status.js"
    },
    {
      "source": "/api/super-admin/global-email/test",
      "destination": "/api/email-test.js"
    },
    {
      "source": "/api/global-email/send-verification",
      "destination": "/api/email-send-verification.js"
    },
    {
      "source": "/api/global-email/send-communication",
      "destination": "/api/email-send-communication.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}