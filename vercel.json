{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/api/companies(.*)",
      "dest": "/api/bookings"
    },
    {
      "src": "/api/venues-with-spaces(.*)",
      "dest": "/api/venues"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}